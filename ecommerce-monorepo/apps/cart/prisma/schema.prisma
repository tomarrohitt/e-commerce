generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma-client"

}

datasource db {
  provider = "postgresql"
}

model ProductReplica {
  id            String   @id
  name          String
  price         Decimal  @db.Decimal(10, 2)
  stockQuantity Int
  thumbnail     String?
  isActive      Boolean
  updatedAt     DateTime @updatedAt

  @@map("product_replicas")
}
